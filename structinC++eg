#include<iostream>
#include<string>

using namespace std;
struct phone{
   string name;
   string processor;
   string Ram;
   string Rom;
   string camera;
   float price; 
};

int main(){
    phone pVar;
    pVar.name="Realme";
    pVar.camera="48MP";
    pVar.price=20000.00;
    pVar.processor="Snapdragon 778";
    pVar.Ram="4GB";
    pVar.Rom="128GB";
  
  cout<<endl<<"Name="<<pVar.name;
  cout<<endl<<"camera="<<pVar.camera;
  cout<<endl<<"price="<<pVar.price;
  cout<<endl<<"Processor="<<pVar.processor;
  cout<<endl<<"Ram="<<pVar.Ram;
  cout<<endl<<"Rom="<<pVar.Rom;

  return 0;
  


}
